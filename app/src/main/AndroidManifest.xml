<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.epicodus.groupfund">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.HomeActivity" />
        <activity android:name=".ui.NewEventTitleActivity" />
        <activity android:name=".ui.InviteActivity" />
        <activity android:name=".ui.NewEventStartDateActivity" />
        <activity android:name=".ui.NewEventEndDateActivity" />
        <activity android:name=".ui.NewEventLocationActivity" />
        <activity android:name=".ui.NewEventDescriptionActivity" />
        <activity android:name=".ui.NewEventTotalCostActivity"></activity>
    </application>

</manifest>